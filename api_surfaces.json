{
  "internal_rpc": {
    "base_path": "/v1/rpc",
    "stability": "internal_only",
    "breaking_changes": "allowed_with_coordination",
    "routing": {
      "selector": "rpc_method_prefix",
      "rules": [
        {
          "prefix": "cloudflare_r2_",
          "upstream": "cloudflare-r2-service"
        },
        {
          "prefix": "github_",
          "upstream": "github-service"
        },
        {
          "prefix": "google_drive_",
          "upstream": "google-drive-service"
        }
      ],
      "fallback_upstream": "ddd-master-editor-backend (temporary)"
    }
  },
  "public_api": {
    "enabled": false,
    "base_path": "/public",
    "versioning": "semver_or_path_versioning",
    "compatibility_contract": "required_before_enable"
  }
}