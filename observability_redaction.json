{
  "drop_headers": [
    "authorization",
    "cookie",
    "set-cookie"
  ],
  "redact_headers": [
    "x-idempotency-key"
  ],
  "redact_fields": [
    "email",
    "display_name"
  ],
  "redact_strategy": {
    "default": "hash",
    "hash_algorithm": "sha256",
    "salt_source": "deployment_secret"
  }
}