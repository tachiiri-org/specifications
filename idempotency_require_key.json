{
  "apply_to_methods": [
    "POST",
    "PUT",
    "PATCH",
    "DELETE"
  ],
  "require_header_on_route_operations": [
    "create",
    "charge",
    "provision"
  ],
  "missing_key": {
    "action": "reject",
    "status": 400,
    "error_code": "idempotency_key_required",
    "message": "x-idempotency-key is required for this operation"
  }
}