{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "spec_version": {
      "type": "integer",
      "minimum": 1
    },
    "breaking_change": {
      "type": "boolean"
    },
    "effective_from": {
      "type": "string",
      "pattern": "^(\\d{4}-\\d{2}-\\d{2}|release-[A-Za-z0-9._-]+)$"
    },
    "boundary": {
      "type": "string",
      "minLength": 1
    },
    "deployment": {
      "type": "object"
    },
    "http": {
      "type": "object",
      "properties": {
        "retry": {
          "type": "object",
          "properties": {
            "mode": {
              "type": "string"
            },
            "apply_to_methods": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "max_attempts": {
              "type": "integer",
              "minimum": 1
            },
            "retry_on_status": {
              "type": "array",
              "items": {
                "type": "integer"
              }
            },
            "retry_on_network_error": {
              "type": "boolean"
            },
            "backoff_ms": {
              "type": "array",
              "items": {
                "type": "number"
              }
            },
            "retry_gate": {
              "type": "object"
            }
          },
          "additionalProperties": true
        },
        "errors": {
          "type": "object",
          "properties": {
            "propagation": {
              "type": "object",
              "properties": {
                "always_use_error_shape": {
                  "type": "boolean"
                },
                "algorithm": {
                  "const": "preserve_then_map_then_shape"
                },
                "preserve_status_for": {
                  "type": "array",
                  "items": {
                    "type": "integer"
                  }
                },
                "map_4xx_to": {
                  "type": "integer"
                },
                "map_5xx_to": {
                  "type": "integer"
                },
                "network_error_to": {
                  "type": "integer"
                }
              },
              "required": [
                "algorithm"
              ],
              "additionalProperties": true
            }
          },
          "additionalProperties": true
        },
        "timeouts": {
          "type": "object",
          "properties": {
            "upstream_timeout_ms": {
              "type": "number"
            },
            "client_timeout_ms": {
              "type": "number"
            }
          },
          "required": [
            "upstream_timeout_ms"
          ],
          "additionalProperties": true
        },
        "contract_version": {
          "type": "object",
          "properties": {
            "mode": {
              "type": "string"
            },
            "header": {
              "type": "string"
            },
            "accepted": {
              "type": "object",
              "properties": {
                "mode": {
                  "type": "string"
                },
                "items": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  },
                  "minItems": 1
                }
              },
              "required": [
                "mode"
              ],
              "additionalProperties": true
            },
            "on_missing": {
              "type": "object"
            }
          },
          "additionalProperties": true
        }
      },
      "additionalProperties": true
    },
    "auth": {
      "type": "object"
    },
    "authz": {
      "type": "object",
      "properties": {
        "mode": {
          "type": "string"
        },
        "pdp": {
          "type": "string"
        },
        "pep": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "minItems": 1
        },
        "decision_context": {
          "type": "object"
        },
        "catalog_binding": {
          "type": "object"
        },
        "on_denied": {
          "type": "object"
        },
        "observability": {
          "type": "object"
        }
      },
      "additionalProperties": true
    },
    "headers": {
      "type": "object",
      "properties": {
        "pipeline_order": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "string"
          }
        }
      },
      "required": [
        "pipeline_order"
      ],
      "additionalProperties": true
    },
    "observability": {
      "type": "object"
    },
    "limits": {
      "type": "object"
    },
    "cors": {
      "type": "object"
    },
    "cookies": {
      "type": "object"
    }
  },
  "required": [
    "spec_version",
    "breaking_change",
    "effective_from",
    "boundary",
    "deployment",
    "http",
    "auth",
    "headers",
    "observability",
    "limits"
  ],
  "additionalProperties": false,
  "allOf": [
    {
      "if": {
        "properties": {
          "http": {
            "type": "object",
            "properties": {
              "contract_version": {
                "type": "object",
                "properties": {
                  "mode": {
                    "const": "required"
                  }
                }
              }
            }
          }
        }
      },
      "then": {
        "properties": {
          "http": {
            "type": "object",
            "properties": {
              "contract_version": {
                "type": "object",
                "required": [
                  "accepted",
                  "header"
                ],
                "properties": {
                  "header": {
                    "type": "string"
                  },
                  "accepted": {
                    "type": "object",
                    "required": [
                      "mode"
                    ],
                    "properties": {
                      "mode": {
                        "type": "string"
                      },
                      "items": {
                        "type": "array",
                        "items": {
                          "type": "string"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    {
      "if": {
        "properties": {
          "http": {
            "type": "object",
            "properties": {
              "contract_version": {
                "type": "object",
                "properties": {
                  "accepted": {
                    "type": "object",
                    "properties": {
                      "mode": {
                        "const": "explicit_list"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "then": {
        "properties": {
          "http": {
            "type": "object",
            "properties": {
              "contract_version": {
                "type": "object",
                "properties": {
                  "accepted": {
                    "type": "object",
                    "required": [
                      "items"
                    ],
                    "properties": {
                      "items": {
                        "type": "array",
                        "minItems": 1,
                        "items": {
                          "type": "string"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  ]
}