{
  "routing": {
    "api_prefix": "/api",
    "healthcheck_paths": [
      "/healthz",
      "/readyz"
    ]
  },
  "methods": {
    "allowed": [
      "GET",
      "POST",
      "PUT",
      "PATCH",
      "DELETE",
      "OPTIONS"
    ],
    "state_changing": [
      "POST",
      "PUT",
      "PATCH",
      "DELETE"
    ]
  },
  "content_types": {
    "accepted_request": [
      "application/json"
    ],
    "require_json_on_methods": [
      "POST",
      "PUT",
      "PATCH",
      "DELETE"
    ],
    "default_response": "application/json; charset=utf-8"
  },
  "auth": {
    "mode": "cookie_session_only",
    "session_cookie_name": "__Host-session",
    "bearer_authorization_header": "disallowed"
  },
  "csrf": {
    "mode": "origin_check",
    "apply_to_methods": [
      "POST",
      "PUT",
      "PATCH",
      "DELETE"
    ]
  },
  "errors": {
    "shape_ref": "response/error.json",
    "catalog_ref": "response/errors.json"
  }
}